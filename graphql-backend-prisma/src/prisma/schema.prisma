generator client {
    provider = "prisma-client-js"
    output   = "../../node_modules/.prisma/client"
}

datasource db {
    provider = "postgresql"
    url      = "postgresql://admin:root@127.0.0.1:5435/graphql-article"
}

model User {
    id    String @id @default(uuid())
    name  String
    email String

    @@map("users")
}
